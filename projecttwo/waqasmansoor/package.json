{
  "name": "todo-global-app",
  "description": "Simple Todo CLI App",
  "version": "1.0.0",
  "author": "Waqas Mansoor @waqasmansoor",
  "bin": {
    "todocli": "./bin/run"
  },
  "bugs": "https://github.com/waqasmansoor/todocli/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "cli-table": "^0.3.6",
    "inquirer": "^8.1.2",
    "lowdb": "^1.0.0",
    "table": "^6.7.1",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/inquirer": "^7.3.3",
    "@types/lowdb": "^1.0.11",
    "@types/node": "^10.17.60",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/waqasmansoor/todocli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "todocli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "waqasmansoor/todocli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
