{
  "name": "abdullah-cli",
  "description": "A Todo CLI App made with oclif and TypeScript for BootCamp 2021",
  "version": "0.0.0",
  "author": "Abdullah Bin Tahir @shy-tan",
  "bin": {
    "abdullah-cli": "./bin/run"
  },
  "bugs": "https://github.com/shy-tan/abdullah-cli/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/node": "^10.17.60",
    "globby": "^10.0.2",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/shy-tan/abdullah-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "abdullah-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "shy-tan/abdullah-cli",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "chalk": "^2.4.2",
    "tslib": "^1.14.1"
  }
}
